script.
  $(document).ready(function(){
    $("#paletteTitle").click(function(){
      //$("#paletteElementScroll").slideToggle("fast");
      console.log($("#navigation").css("height"));
      if($("#navigation").css("height") === "600px")
        $("#navigation").animate({"height": "50px"}, 100);
      else
        $("#navigation").animate({"height": "600px"}, 100);
    });

    $("#paletteTitle_input").click(function(){
      $("#paletteElementScroll_input").slideToggle("fast");
    });

    $("#paletteTitle_output").click(function(){
