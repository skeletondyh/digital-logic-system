style.
  body {
      margin: 0;
      padding: 0;
      overflow: hidden;
      background-color: #F0F0F0;
  }

  .form-canvas {
      position: absolute;
      top: 55px;
      left: 10px;
  }
  .modal-xs {
      width: 200px;
  }
  .box {
      width:305px; /* or whatever width you want. */
      max-width:3000px; /* or whatever width you want. */
      display: inline-block;
      position: absolute;
      top: 55px;
      right: 10px;
  }
  .box-title {
      display: inline-block;
      padding-top: 0px;
      padding-right: 0px;
      padding-bottom: 0px;
      padding-left: 0px;
      font-size: 14px;
      line-height: 1;
  }
  .btn-default:focus {
      background-color: #fff;
      border-color: #ccc;
  }
  .chip-list {
      overflow-y: scroll;
  }
  .chip-card {
      text-align: left;
      display: block;
      width: 100%;
      border-width: 0;
  }
  .zoom-container {
      width:115px;
      max-width:120px;
      display: inline-block;
      position:absolute;
      bottom:12px;
      right:12px;
  }
  /* TODO: create my own style for these styles */
  p.lead {
      margin-bottom: 8px;
  }
  hr {
      margin: 0;
  }
  .btn-group-vertical {
      display: inline
  }
  .my-panel-body {
      padding: 8px 12px
  }
  .my-panel-heading {
      padding: 8px 12px;
  }
  .my-panel-footer {
      padding: 1px 12px
  }



